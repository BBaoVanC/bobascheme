    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5e5e5e"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#d4d4d4"
            }
        },
        {
            "name": "Colors",
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#d4d4d4"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#b04744"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "keyword.other.unit",
                "keyword.other",
                "keyword.control",
                "entity.name.type.lifetime",
                "punctuation.definition.lifetime"
            ],
            "settings": {
                "foreground": "#9f4b95"
            }
        },
        {
            "name": "Operator, Misc",
            "scope": [
                "keyword.operator",
                "constant.other.color",
                // "punctuation",
                // "meta.tag",
                // "punctuation.definition.tag",
                // "punctuation.separator.inheritance.php",
                // "punctuation.definition.tag.html",
                // "punctuation.definition.tag.begin.html",
                // "punctuation.definition.tag.end.html",
                // "punctuation.section.embedded",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#d4d4d4",
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
            ],
            "settings": {
                "foreground": "#d4d4d4",
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#29757f"
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#29757f"
            }
        },
        {
            "name": "Block Level Variables",
            "scope": [
                "meta.block variable.other"
            ],
            "settings": {
                "foreground": "#d4d4d4"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": [
                "support.other.variable",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#00ffff"
            }
        },
        {
            "name": "Macros",
            "scope": [
                "entity.name.function.macro"
            ],
            "settings": {
                "foreground": "#b04744",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "meta.attribute.rust",
                "meta.attribute.rust punctuation",
                "meta.attribute.rust keyword.operator"
            ],
            "settings": {
                "foreground": "#b04744",
            }
        },
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded, Escape",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#8c6110"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#167a42"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
                "support.other.namespace.use.php",
                "meta.use.php",
                "support.other.namespace.php",
                "markup.changed.git_gutter",
                "support.type.sys-types"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#216dbf"
            }
        },
        {
            "name": "Make lifetimes not bold",
            "scope": [
                "entity.name.type.lifetime"
            ],
            "settings": {
                "fontStyle": ""
            }
        },
        {
